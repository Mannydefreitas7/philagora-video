<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Aperture</string>
	<key>CFBundleDocumentTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeName</key>
			<string>Video</string>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>LSHandlerRank</key>
			<string>Alternate</string>
			<key>LSItemContentTypes</key>
			<array>
				<string>public.movie</string>
				<string>public.mpeg-4</string>
				<string>com.apple.quicktime-movie</string>
				<string>public.avi</string>
				<string>com.compuserve.gif</string>
			</array>
		</dict>
	</array>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>com.philagora.aperture</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Aperture</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundlePrimaryIcon</key>
	<string>AppIcon</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0.0</string>
	<key>CFBundleVersion</key>
	<string>1.0.0</string>
	<key>LSApplicationCategoryType</key>
	<string>public.app-category.video</string>
	<key>LSMinimumSystemVersion</key>
	<string>26.0</string>
	<key>LSUIElement</key>
	<false/>
	<key>MetalCaptureEnabled</key>
	<true/>
	<key>NSAppleMusicUsageDescription</key>
	<string>Aperture may access your music library for audio processing.</string>
	<key>NSCameraUsageDescription</key>
	<string>Aperture needs camera access to record video from your camera.</string>
	<key>NSDesktopFolderUsageDescription</key>
	<string>Aperture needs access to save recordings to your Desktop.</string>
	<key>NSDocumentsFolderUsageDescription</key>
	<string>Aperture needs access to save recordings to your Documents.</string>
	<key>NSDownloadsFolderUsageDescription</key>
	<string>Aperture needs access to save recordings to your Downloads.</string>
	<key>NSHighResolutionCapable</key>
	<true/>
	<key>NSMainStoryboardFile</key>
	<string></string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Aperture needs microphone access to record audio.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Aperture needs access to your Photos library to save recorded videos and export them to your photo collection.</string>
	<key>NSPrincipalClass</key>
	<string>NSApplication</string>
	<key>NSRequiresAquaSystemAppearance</key>
	<false/>
	<key>NSScreenCaptureUsageDescription</key>
	<string>Aperture needs screen recording permission to capture your screen.</string>
	<key>NSSupportsAutomaticGraphicsSwitching</key>
	<true/>
	<key>NSSupportsAutomaticTermination</key>
	<true/>
	<key>NSSupportsSuddenTermination</key>
	<true/>
	<key>UIBackgroundModes</key>
	<array>
		<string>audio</string>
		<string>background-processing</string>
	</array>
	<key>UTImportedTypeDeclarations</key>
	<array>
		<dict/>
	</array>
